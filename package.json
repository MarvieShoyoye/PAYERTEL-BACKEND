{
  "name": "payertel-api",
  "version": "1.0.0",
  "description": "Learning Node.js, Express, and MongoDB",
  "type": "module",
  "main": "server.js",
  "targets": {
    "main": false
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "prod": "cross-env NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url .",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "author": "Payertel",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.13",
    "nodemon": "^3.1.4",
    "parcel": "^2.12.0",
    "path": "^0.12.7",
    "pug": "^3.0.3",
    "slugify": "^1.6.6",
    "validator": "^13.12.0",
    "xss-clean": "^0.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
